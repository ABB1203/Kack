package game.entity.mob.AI;

import game.entity.mob.Mob;

public class AI extends Mob {

	public AI() {
		sprite = sprite.voidSprite;
	}

	public void tick() {
		super.tick();

		x--;
	}

	protected int randomTicks() {
		// This gives a random value to how many ticks the AI should wait. If
		// not updated, th number of ticks should be 60 each second
		return 60 + random.nextInt(60);

	}

}
